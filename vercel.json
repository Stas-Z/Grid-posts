{
	"version": 2,
	"builds": [
		{
			"src": "client/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "client/build"
			}
		},
		{
			"src": "server/dist/main.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{ "src": "/api/(.*)", "dest": "server/dist/main.js" },
		{ "src": "/(.*)", "dest": "client/out/$1" }
	]
}
